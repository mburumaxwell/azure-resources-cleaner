{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "AzureCleaner": {
      "commandName": "Project",
      //"commandLineArgs": "AzureCleaner --pr 3001 --subscription DEPENDABOT --dry-run",
      "launchBrowser": true,
      "launchUrl": "health",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DOTNET_ENVIRONMENT": "Development",
        "AS_WEB_APP": "true"
      },
      "applicationUrl": "https://localhost:44392;http://localhost:59271"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/health",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080",
        "DOTNET_ENVIRONMENT": "Development",
        "AS_WEB_APP": "true"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}